{
  "_version": "1.12.0",
  "sap.app": {
    "id": "capcom.recap",
    "type": "application",
    "title": "re>â‰¡CAP Conference Website",
    "i18n": {
      "bundleUrl": "i18n/i18n.properties",
      "supportedLocales": ["en"],
      "fallbackLocale": "en"
    },
    "applicationVersion": {
      "version": "1.0.3"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "resources": {
      "css": [{ "uri": "styles.css" }]
    },
    "rootView": {
      "viewName": "capcom.recap.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.68.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "Agenda": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "Agenda.json"
      },
      "AgendaOnline": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "AgendaOnline.json"
      },
      "OurSponsors": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "OurSponsors.json"
      }
